<core:FragmentDefinition xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog id="editDialog" title="create">
        <form:SimpleForm backgroundDesign="Transparent">
            <Label text="Userid"/>
            <Input value="{editdata>/Userid}" id="Userid3edit"  liveChange="liveChangeFirstname"/>
            <Label text="Username"/>
            <Input value="{editdata>/Username}" id="Username3edit"  liveChange="liveChangeLastname"/>
            <Label text="Useremail"/>
            <Input value="{editdata>/Useremail}" id="Useremail3edit"  liveChange="liveChangeEmail"/>
            <Label text="Userpass"/>
            <Input value="{editdata>/Userpass}" id="Userpass3edit"  liveChange="liveChangePassword"/>
            <Label text="Userdesign"/>
            <Input value="{editdata>/Userdesign}" id="Userdesign3edit"  liveChange="liveChangePassword"/>
          
        </form:SimpleForm>
        <beginButton>
            <Button text="Close" press=".onCloseDialog"/>
        </beginButton>
        <endButton>
            <Button text="Add" press=".oneditDetails"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>